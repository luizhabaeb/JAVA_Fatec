package dto;

// Atributos herdados da Classe mãe Cliente
public class Funcionario extends Cliente {

	
	private String cargo;
	
	public void Cliente(String cargo) {
		
		this.cargo = cargo;
	}
	
	public String getCargo() {
		return cargo;
	}

	public void setCargo(String cargo) {
		this.cargo = cargo;
	}

